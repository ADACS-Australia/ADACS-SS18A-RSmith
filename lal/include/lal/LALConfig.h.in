/* only include this file if LAL's config.h has not been included */
#ifndef CONFIG_H__LAL

#ifndef _LALCONFIG_H
#define _LALCONFIG_H

/* LAL Version */
#undef LAL_VERSION

/* LAL Version Major Number  */
#undef LAL_VERSION_MAJOR

/* LAL Version Minor Number  */
#undef LAL_VERSION_MINOR

/* LAL Configure Arguments */
#undef LAL_CONFIGURE_ARGS

/* LAL Configure Date */
#undef LAL_CONFIGURE_DATE

/* The number of bytes in a double.  */
#undef LAL_SIZEOF_DOUBLE

/* The number of bytes in a float.  */
#undef LAL_SIZEOF_FLOAT

/* The number of bytes in a int.  */
#undef LAL_SIZEOF_INT

/* The number of bytes in a long.  */
#undef LAL_SIZEOF_LONG

/* The number of bytes in a long long.  */
#undef LAL_SIZEOF_LONG_LONG

/* The number of bytes in a short.  */
#undef LAL_SIZEOF_SHORT

/* Suppress debugging code */
#undef LAL_NDEBUG

/* Use functions rather than macros */
#undef NOLALMACROS

/* Use pthread mutex lock for threadsafety */
#undef LAL_PTHREAD_LOCK

#endif /* _LALCONFIG_H */

#endif /* CONFIG_H__LAL */
