#undef LAL_NDEBUG

/* Use functions rather than macros */
#undef NOLALMACROS

/* Use pthread mutex lock for threadsafety */
#undef LAL_PTHREAD_LOCK

/* Define if using fftw3 library */
#undef LAL_FFTW3_ENABLED

/* Define if using Frame library */
#undef LAL_FRAME_ENABLED

/* Define if using MPI */
#undef LAL_MPI_ENABLED

/* Define if using MetaIo library */
#undef LAL_METAIO_ENABLED

/* Define if using qthread library */
#undef LAL_QTHREAD

#endif /* _LALCONFIG_H */

#endif /* CONFIG_H__LAL */
