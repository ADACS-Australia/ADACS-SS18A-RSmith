universe = standard
executable = executables/lalapps_inspiral
arguments = --trig-end-time $(macrotrigendtime) --cluster-method template --dynamic-range-exponent 69.0 --disable-rsq-veto --bank-file $(macrobankfile) --high-pass-order 8 --strain-high-pass-order 8 --ifo-tag $(macroifotag) --approximant FindChirpSP --gps-end-time $(macrogpsendtime) --calibrated-data real_8 --channel-name $(macrochannelname) --snr-threshold 5.5 --number-of-segments 15 --trig-start-time $(macrotrigstarttime) --enable-high-pass 30.0 --debug-level 33 --gps-start-time $(macrogpsstarttime) --enable-filter-inj-only --high-pass-attenuation 0.1 --chisq-bins 0 --inverse-spec-length 16 --segment-length 1048576 --low-frequency-cutoff 40.0 --pad-data 8 --sample-rate 4096 --chisq-threshold 10.0 --resample-filter ldas --strain-high-pass-atten 0.1 --strain-high-pass-freq 30 --injection-file HLV-INJ001.xml --segment-overlap 524288 --frame-cache $(macroframecache) --chisq-delta 0.2 --bank-veto-subbank-size 1 --user-tag $(macrousertag) --ts-metric-scaling 0.06 --write-compress --enable-output --ts-cluster T0T3TcAS --order twoPN --spectrum-type median
environment = KMP_LIBRARY=serial;MKL_SERIAL=yes
log = /people/jclayton/logs/RmpMoimny
error = logs/inspiral-$(macrogpsstarttime)-$(macrogpsendtime)-$(cluster)-$(process).err
output = logs/inspiral-$(macrogpsstarttime)-$(macrogpsendtime)-$(cluster)-$(process).out
notification = never
queue 1
