#!/bin/bash

for filename in "${@}" ; do
	echo ${filename}
	tmp=$(mktemp)
	{ gzip -d <${filename} | ligolw_cut --delete-table time_slide --delete-table coinc_event --delete-table coinc_event_map --delete-table coinc_definer --delete-table multi_burst | sed -e "/ligolw_burca/d" -e "/ligolw_binjfind/d" -e "/ligolw_tisi/d" -e "/ligolw_bucut/d" | gzip >${tmp} && mv -f ${tmp} ${filename} ; } || break
done
