#!/bin/bash

for filename in "${@}" ; do
	echo ${filename}
	tmp=$(mktemp)
	gzip -d <${filename} | ligolw_cut --delete-table time_slide --delete-table coinc_event --delete-table coinc_event_map --delete-table coinc_definer | sed -e "/ligolw_burca/d" -e "/ligolw_binjfind/d" -e "/ligolw_tisi/d" | gzip >${tmp} && mv -f ${tmp} ${filename}
done
