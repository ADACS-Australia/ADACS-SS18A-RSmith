#define LALMETAIO_VCS_IDENT_STATUS "$" "LALMetaIOStatus: @STATUS@ " "$"

/* vcs information structures */
static const struct tagLALVCSInfo lalMetaIOHeaderVCSInfo = { \
  LALMETAIO_VCS_NAME, \
  LALMETAIO_VERSION, \
  LALMETAIO_VCS_ID, \
  LALMETAIO_VCS_DATE, \
