#ifndef _LALMETAIOVCSINFO_H
#define _LALMETAIOVCSINFO_H

#include <lal/LALVCSInfo.h>
#include <lal/LALMetaIOLibVCSInfo.h>

#ifdef __cplusplus
extern "C" {
#endif

/* vcs information defines */
#define LALMETAIO_VCS_NAME "LALMetaIO"
#define LALMETAIO_VCS_ID "@ID@"
#define LALMETAIO_VCS_DATE "@DATE@"
#define LALMETAIO_VCS_BRANCH "@BRANCH@"
#define LALMETAIO_VCS_TAG "@TAG@"
#define LALMETAIO_VCS_AUTHOR "@AUTHOR@"
#define LALMETAIO_VCS_COMMITTER "@COMMITTER@"
